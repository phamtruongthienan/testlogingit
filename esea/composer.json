{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-translation-manager": "^0.5.2",
        "cocur/slugify": "^3.1",
        "consoletvs/charts": "6.*",
        "cviebrock/eloquent-sluggable": "4.6",
        "davejamesmiller/laravel-breadcrumbs": "5.x",
        "dimsav/laravel-translatable": "^9.2",
        "doctrine/instantiator": "~1.0.3",
        "fideloper/proxy": "^4.0",
        "greggilbert/recaptcha": "dev-master",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "^2.4",
        "jenssegers/agent": "^2.6",
        "jenssegers/date": "^3.4",
        "jordanmiguel/laravel-popular": "^1.0",
        "laravel/framework": "5.7.*",
        "laravel/socialite": "^3.2",
        "laravel/tinker": "^1.0",
        "laravelium/sitemap": "3.0.*",
        "league/glide-laravel": "^1.0",
        "maatwebsite/excel": "^3.1",
        "mariuzzo/laravel-js-localization": "^1.4",
        "mcamara/laravel-localization": "^1.3",
        "renatomarinho/laravel-page-speed": "^1.8",
        "shawnsandy/img-fly": "^0.1.21",
        "spatie/laravel-activitylog": "^3.1",
        "spatie/laravel-analytics": "^3.6",
        "spatie/laravel-query-builder": "^1.11",
        "staudenmeir/eloquent-json-relations": "^1.0",
        "teamtnt/laravel-scout-tntsearch-driver": "^3.3",
        "yajra/laravel-datatables-oracle": "~8.0",
        "zizaco/entrust": "5.2.x-dev"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0",
        "reliese/laravel": "^0.0.13"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
